{
    "name": "remyhonig/git-presented",
    "description": "A Jigsaw-based static site generator that turns a Git repository into a presentation website.",
    "keywords": [
        "git",
        "presentation",
        "static",
        "site",
        "generator",
        "jigsaw"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "league/commonmark": "^2.8",
        "sebastian/diff": "^6.0",
        "tightenco/jigsaw": "^1.8"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
